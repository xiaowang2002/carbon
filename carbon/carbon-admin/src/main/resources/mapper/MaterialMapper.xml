<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zhonghui.web.mapper.MaterialMapper">

    <select id="getMaterialListByClassifyId" resultType="com.zhonghui.web.pojo.Material">
        select *
        from wms_material
        where classify_id = #{id}
    </select>
</mapper>